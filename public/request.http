### GET DEFAULT INFO
GET {{baseurl}}/api/page
Authorization: Bearer {{token}}


### GET TOKENS VIA GOOGLE TOKEN
POST {{baseurl}}/api/auth/googlelogin
Content-Type: application/json

{
  "IdToken" : "{{idtoken}}"
}


### GET NEW SET OF TOKENS
POST {{baseurl}}/api/auth/refreshtoken
Content-Type: application/json

{
  "token": "{{token}}",
  "refreshToken": "{{refreshtoken}}"
}



### PAGE FUNCTIONS
### GET ALL DATA FOR A PAGE
GET {{baseurl}}/api/page/10
Authorization: Bearer {{token}}

### CREATE NEW PAGE
POST {{baseurl}}/api/page
Content-Type: application/json
Authorization: Bearer {{token}}

{
  "title" : "ek bada page ka naam"
}

### DELETE PAGE
DELETE {{baseurl}}/api/page
Authorization: Bearer {{token}}
Content-Type: application/json

{
  "id" : "5"
}

### UPDATE PAGE
PUT {{baseurl}}/api/page
Authorization: Bearer {{token}}
Content-Type: application/json

{
  "page" : {
    "title" : "newTitleUpdated",
    "page_id" : "5"
  }
}

### COLLECTION FUNCTIONS
### CREATE NEW COLLECTION
POST {{baseurl}}/api/collection
Content-Type: application/json
Authorization: Bearer {{token}}

{
  "title" : "coll 1",
  "pageid" : "55"
}

### DELETE COLLECTION
DELETE {{baseurl}}/api/collection
Authorization: Bearer {{token}}
Content-Type: application/json

{
  "id" : "20",
  "pageid" : "5"
}

### UPDATE COLLECTION
PUT {{baseurl}}/api/collection
Authorization: Bearer {{token}}
Content-Type: application/json

{
  "collection" : {
    "collection_id" : "22",
    "title" : "new updated collection title"
  },
  "pageid" : "6"
}

